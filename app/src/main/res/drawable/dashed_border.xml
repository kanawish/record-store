<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:bottom="2dp"
        android:left="2dp"
        android:right="2dp"
        android:top="2dp">
        <shape>
            <stroke
                android:width="2dp"
                android:color="#bbbbbb"
                android:dashWidth="10dp"
                android:dashGap="6dp" />
            <corners android:radius="4dp" />
        </shape>
    </item>
</layer-list>
